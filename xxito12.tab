create table SCOTT.XXITO12
(
  usl    CHAR(3) not null,
  org    NUMBER(3) not null,
  trest  CHAR(2) not null,
  reu    CHAR(3) not null,
  summa  NUMBER(10,2) not null,
  dat    DATE,
  mg     CHAR(6),
  var    NUMBER not null,
  forreu CHAR(3) not null,
  status NUMBER not null,
  dopl   CHAR(6) not null,
  kul    CHAR(4),
  nd     CHAR(6)
)
tablespace USERS
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 5M
    next 1M
    minextents 1
    maxextents unlimited
  )
nologging;
