create unique index SCOTT.SPR_PROC_PAY_I on SCOTT.SPR_PROC_PAY (USL, ORG, REU, MG1, MG2)
  tablespace USERS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );

