create table SCOTT.XXITO14_LSK
(
  summa  NUMBER(10,2) not null,
  usl    CHAR(3) not null,
  org    NUMBER(3) not null,
  dat    DATE,
  mg     CHAR(6),
  var    NUMBER not null,
  status NUMBER not null,
  dopl   CHAR(6) not null,
  oper   CHAR(2),
  lsk    CHAR(8) not null,
  cd_tp  NUMBER
)
tablespace USERS
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 22M
    next 1M
    minextents 1
    maxextents unlimited
  );
comment on table SCOTT.XXITO14_LSK
  is 'Оплата по предпр., трестам, услугам / статусу жилья/ коду операции';
comment on column SCOTT.XXITO14_LSK.cd_tp
  is 'Тип (1-оплата,0 - пеня)';
create index SCOTT.XXITO14_LSK_I_DAT on SCOTT.XXITO14_LSK (DAT)
  tablespace USERS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
create index SCOTT.XXITO14_LSK_I_LSK on SCOTT.XXITO14_LSK (LSK)
  tablespace USERS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 20M
    next 1M
    minextents 1
    maxextents unlimited
  );

