comment on column SCOTT.A_NABOR_PROGS.usl
  is 'ID услуги';
comment on column SCOTT.A_NABOR_PROGS.fk_tarif
  is 'ID Тариф (Антенн)';
comment on column SCOTT.A_NABOR_PROGS.mg
  is 'Период';

