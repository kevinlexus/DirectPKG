  dopl      CHAR(6) not null,
  nink      NUMBER,
  nkom      CHAR(3) not null,
  dtek      DATE not null,
  nkvit     NUMBER,
  dat_ink   DATE,
  ts        DATE,
  c_kwtp_id NUMBER not null,
  rasp_id   NUMBER,
  mg        CHAR(6) not null,
  cnt_sch   NUMBER,
