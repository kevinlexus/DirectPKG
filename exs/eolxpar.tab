create table EXS.EOLXPAR
(
  id        NUMBER not null,
  fk_eolink NUMBER not null,
  fk_par    NUMBER not null,
  n1        NUMBER,
  s1        VARCHAR2(1024),
  d1        DATE
)
tablespace DATA
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
comment on table EXS.EOLXPAR
  is 'Параметры внешнего объекта';
comment on column EXS.EOLXPAR.fk_eolink
  is 'FK на внешний объект';
comment on column EXS.EOLXPAR.fk_par
  is 'FK на параметр ассоциированный с ГИС ЖКХ';
comment on column EXS.EOLXPAR.n1
  is 'Значение параметра типа Number';
comment on column EXS.EOLXPAR.s1
  is 'Значение параметра типа Varchar2';
comment on column EXS.EOLXPAR.d1
  is 'Значение параметра типа Date';
create unique index EXS.EOLXPAR_I on EXS.EOLXPAR (FK_EOLINK, FK_PAR)
  tablespace INDX_FAST
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
alter table EXS.EOLXPAR
  add constraint EOLXPAR_P primary key (ID)
  using index 
  tablespace DATA
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
alter table EXS.EOLXPAR
  add constraint EOLXPAR_F foreign key (FK_EOLINK)
  references EXS.EOLINK (ID) on delete cascade;

