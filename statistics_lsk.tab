create table SCOTT.STATISTICS_LSK
(
  usl        CHAR(3),
  reu        CHAR(3) not null,
  kul        CHAR(4) not null,
  nd         CHAR(6) not null,
  kw         CHAR(7) not null,
  cnt        NUMBER,
  klsk       NUMBER,
  kpr        NUMBER,
  kpr_ot     NUMBER,
  kpr_wr     NUMBER,
  mg         VARCHAR2(6),
  org        NUMBER(3),
  status     NUMBER(1),
  psch       NUMBER(1),
  sch        NUMBER,
  cnt_lg     NUMBER,
  cnt_subs   NUMBER,
  dat        DATE,
  val_group  VARCHAR2(20),
  uch        NUMBER,
  lsk        CHAR(8),
  val_group2 VARCHAR2(20),
  cnt_room   NUMBER,
  fio        VARCHAR2(55),
  cena       NUMBER,
  limit      NUMBER,
  is_empt    NUMBER,
  fk_tp      NUMBER,
  opl        NUMBER,
  is_vol     VARCHAR2(20),
  chng_vol   NUMBER
)
tablespace USERS
  pctfree 1
  initrans 1
  maxtrans 255
  storage
  (
    initial 182M
    next 1M
    minextents 1
    maxextents unlimited
  );
comment on table SCOTT.STATISTICS_LSK
  is 'Статистика по услугам с детализацией по квартирам';
comment on column SCOTT.STATISTICS_LSK.usl
  is 'ID услуги';
comment on column SCOTT.STATISTICS_LSK.reu
  is '№ РЭУ';
comment on column SCOTT.STATISTICS_LSK.kul
  is 'ID улицы';
comment on column SCOTT.STATISTICS_LSK.nd
  is '№ дома';
comment on column SCOTT.STATISTICS_LSK.kw
  is '№ квариры';
comment on column SCOTT.STATISTICS_LSK.cnt
  is 'Предоставленный объем услуги (квт., куб., точ., чел., м2)';
comment on column SCOTT.STATISTICS_LSK.klsk
  is 'Кол-во лицевых счетов';
comment on column SCOTT.STATISTICS_LSK.kpr
  is 'Кол-во прописанных';
comment on column SCOTT.STATISTICS_LSK.kpr_ot
  is 'Кол-во временно отсутствующих';
comment on column SCOTT.STATISTICS_LSK.kpr_wr
  is 'Кол-во временно зарегистрированных';
comment on column SCOTT.STATISTICS_LSK.mg
  is 'Дата формирования статистики (ггггмм)';
comment on column SCOTT.STATISTICS_LSK.org
  is 'Код организации - поставщика услуги';
comment on column SCOTT.STATISTICS_LSK.status
  is 'Статус жилья';
comment on column SCOTT.STATISTICS_LSK.psch
  is 'Признак закрытых лицевых счетов (1-закрытые, 0-действующие)';
comment on column SCOTT.STATISTICS_LSK.sch
  is 'Признак наличия индивидуального счетчика (водоснабжение, электроснабжение) 0 -без сч. 1-счетчик.';
comment on column SCOTT.STATISTICS_LSK.cnt_lg
  is 'Кол-во льготников, пользующихся услугой';
comment on column SCOTT.STATISTICS_LSK.cnt_subs
  is 'Кол-во лицевых, пользующихся услугой, имеющих субсидию';
comment on column SCOTT.STATISTICS_LSK.dat
  is 'Дата формирования статистики (ддммгггг)';
comment on column SCOTT.STATISTICS_LSK.val_group
  is 'Коэфф по услуге';
comment on column SCOTT.STATISTICS_LSK.uch
  is 'Участок';
comment on column SCOTT.STATISTICS_LSK.val_group2
  is 'Норматив по услуге';
comment on column SCOTT.STATISTICS_LSK.cnt_room
  is 'Кол-во квартир (только для Итогов)';
comment on column SCOTT.STATISTICS_LSK.fio
  is 'ФИО квартиросъемщика';
comment on column SCOTT.STATISTICS_LSK.cena
  is 'Цена';
comment on column SCOTT.STATISTICS_LSK.limit
  is 'Лимит объема по услуге (Используется при начислении по ОДН)';
comment on column SCOTT.STATISTICS_LSK.is_empt
  is 'Пустая ли квартира (0-нет, 1 - да)';
comment on column SCOTT.STATISTICS_LSK.fk_tp
  is 'FK на тип счета (Основоной, Дополнительный)';
comment on column SCOTT.STATISTICS_LSK.opl
  is 'Площадь по услуге (в дополнение к CNT)';
comment on column SCOTT.STATISTICS_LSK.is_vol
  is 'Имеется объем по услуге (null,0-нет, 1 -да)';
comment on column SCOTT.STATISTICS_LSK.chng_vol
  is 'Объем, полученный из перерасчета';
create index SCOTT.STAT_LSK_DAT_REU on SCOTT.STATISTICS_LSK (DAT, REU, KUL, ND, USL, ORG)
  tablespace USERS
  pctfree 0
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
create index SCOTT.STAT_LSK_MG_LSK on SCOTT.STATISTICS_LSK (MG, LSK)
  tablespace USERS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
create index SCOTT.STAT_LSK_MG_REU on SCOTT.STATISTICS_LSK (MG, REU, KUL, ND, USL, ORG)
  tablespace USERS
  pctfree 0
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );

