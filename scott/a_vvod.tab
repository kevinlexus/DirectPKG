create table SCOTT.A_VVOD
(
  house_id           NUMBER,
  id                 NUMBER,
  kub                NUMBER(12,3),
  kub_man            NUMBER(12,3),
  kpr                NUMBER,
  kub_sch            NUMBER,
  sch_cnt            NUMBER,
  sch_kpr            NUMBER,
  cnt_lsk            NUMBER,
  mg                 CHAR(6),
  usl                CHAR(3),
  vvod_num           NUMBER,
  vol_add            NUMBER(12,3),
  sch_add            NUMBER,
  kub_fact           NUMBER(12,3),
  vol_add_fact       NUMBER(12,3),
  itg_fact           NUMBER(12,3),
  opl_add            NUMBER,
  kub_norm           NUMBER,
  kub_nrm_fact       NUMBER,
  kub_sch_fact       NUMBER,
  use_sch            NUMBER,
  dist_tp            NUMBER,
  opl_ar             NUMBER,
  kub_ar             NUMBER,
  kub_ar_fact        NUMBER,
  limit_proc         NUMBER,
  kub_dist           NUMBER,
  wo_limit           NUMBER,
  edt_norm           NUMBER,
  nrm                NUMBER,
  kub_fact_upnorm    NUMBER,
  non_heat_per       NUMBER,
  ishotpipeinsulated NUMBER,
  istowelheatexist   NUMBER
)
tablespace USERS
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 512K
    next 1M
    minextents 1
    maxextents unlimited
  )
nologging;
comment on column SCOTT.A_VVOD.kub_sch
  is 'расход по счетчику по вводу';
comment on column SCOTT.A_VVOD.usl
  is 'ID Услуги, если пусто - значит подъезд';
comment on column SCOTT.A_VVOD.vol_add
  is 'Добавочные кубы на дом по услуге (например доначисление по 307 постановлению)';
comment on column SCOTT.A_VVOD.sch_add
  is 'Доначислять ли добавочные кубы (по 307) там где есть индив.счетчики?(0-нет, 1 -да)';
comment on column SCOTT.A_VVOD.kub_fact
  is 'Дораспределено факт, итого (в пределах нормы)';
comment on column SCOTT.A_VVOD.vol_add_fact
  is 'Распределилось фактически, по вводу (по 307)';
comment on column SCOTT.A_VVOD.itg_fact
  is 'Распределилось фактически, всего, по вводу (по 307)';
comment on column SCOTT.A_VVOD.opl_add
  is 'Распределилось на м2 по вводу (по 307)';
comment on column SCOTT.A_VVOD.kub_norm
  is 'расход по нормативным по дому';
comment on column SCOTT.A_VVOD.kub_nrm_fact
  is 'Кубы по нормативу после распределения по вводу';
comment on column SCOTT.A_VVOD.kub_sch_fact
  is 'Кубы по счетчикам после распределения по вводу';
comment on column SCOTT.A_VVOD.use_sch
  is 'Использовать счетчики для распределения(1-да, 0 - нет)';
comment on column SCOTT.A_VVOD.dist_tp
  is 'Распределение воды по дому (0, null-пропорционально расходу, 1 - проп. площади,
4-по дому, без ОДПУ, есть возм.установки, 5-по дому, без ОДПУ, нет возм.установки,
6-просто учитывать объем)';
comment on column SCOTT.A_VVOD.opl_ar
  is 'Площадь по нежилым';
comment on column SCOTT.A_VVOD.kub_ar
  is 'Объем по нежилым';
comment on column SCOTT.A_VVOD.kub_ar_fact
  is 'Дораспределено~фактически по нежилым';
comment on column SCOTT.A_VVOD.limit_proc
  is '% органичения по доначислению';
comment on column SCOTT.A_VVOD.kub_dist
  is 'Распределенный объём';
comment on column SCOTT.A_VVOD.wo_limit
  is '(1-не ограничивать лимитом потребления ОДН, 0 и null- ограничивать) (долько для определённых dist_tp)';
comment on column SCOTT.A_VVOD.edt_norm
  is 'УДАЛИТЬ! Введённый норматив на м2 (Например для отопления, Полыс. там где отопление в гкал, надо начислять по нормативу, nabor.norm - не подходит для этого, оно в кис. для отопления м2)';
comment on column SCOTT.A_VVOD.nrm
  is 'Норматив по ОДН, для отчётов, рассчитывается либо по таблице opl_liter, либо в условии';
comment on column SCOTT.A_VVOD.kub_fact_upnorm
  is 'Дораспределено факт, итого  (свыше нормы)';
comment on column SCOTT.A_VVOD.non_heat_per
  is 'Начислять ли в неотопительный период? (0-нет, 1-да)';
comment on column SCOTT.A_VVOD.ishotpipeinsulated
  is 'Изолирован ли стояк горячей воды? (для статистики)';
comment on column SCOTT.A_VVOD.istowelheatexist
  is 'Наличие полотенцесушителя';
create index SCOTT.A_VVOD_I on SCOTT.A_VVOD (HOUSE_ID)
  tablespace USERS
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 512K
    next 1M
    minextents 1
    maxextents unlimited
  )
  nologging;

