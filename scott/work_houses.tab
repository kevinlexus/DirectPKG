create table SCOTT.WORK_HOUSES
(
  id         NUMBER not null,
  reu        CHAR(3) not null,
  kul        CHAR(4) not null,
  nd         CHAR(6) not null,
  newreu     CHAR(2),
  uch        NUMBER,
  maxlsk     CHAR(8),
  kw         NUMBER,
  minlsk     CHAR(8),
  house_type NUMBER,
  opl        NUMBER
)
tablespace USERS
  pctfree 1
  initrans 1
  maxtrans 255
  storage
  (
    initial 128K
  )
nologging;
comment on table SCOTT.WORK_HOUSES
  is 'Справочник домов, для подготовки УК';
comment on column SCOTT.WORK_HOUSES.id
  is 'ID дома';
comment on column SCOTT.WORK_HOUSES.uch
  is 'Участок';
comment on column SCOTT.WORK_HOUSES.maxlsk
  is 'Кон. Л.С.';
comment on column SCOTT.WORK_HOUSES.kw
  is 'Кол-во квартир';
comment on column SCOTT.WORK_HOUSES.minlsk
  is 'Нач.Л.С.';
comment on column SCOTT.WORK_HOUSES.house_type
  is 'Тип дома. 1- общежитие';
comment on column SCOTT.WORK_HOUSES.opl
  is 'Общая площадь по дому';

