create global temporary table SCOTT.LIST_CHOICES_HS
(
  kul CHAR(4),
  nd  CHAR(6),
  sel NUMBER(1)
)
on commit preserve rows;
comment on table SCOTT.LIST_CHOICES_HS
  is 'Таблица для выбора домов пользователем';
comment on column SCOTT.LIST_CHOICES_HS.kul
  is 'Id улицы';
comment on column SCOTT.LIST_CHOICES_HS.nd
  is '№ дома';
comment on column SCOTT.LIST_CHOICES_HS.sel
  is 'Отмечено';
create index SCOTT.LIST_CHOICES_HS on SCOTT.LIST_CHOICES_HS (KUL, ND, SEL);

